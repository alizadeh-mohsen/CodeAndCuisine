@model IEnumerable<CodeAndCuisine.Web.Models.ShoppingCartDto>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>CartTotal</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.CartHeader.Id</td>
                <td>@item.CartHeader.CartTotal</td>
            </tr>

        }
    </tbody>
</table>
